import java.util.Scanner;
class Main {

  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("ENTER THE FIRST NUMBER");
    float n=sc.nextInt();
    ystem.out.println("ENTER THE SECOND NUMBER");
